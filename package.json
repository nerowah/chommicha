{
  "name": "nerowah",
  "version": "1.13.0",
  "description": "nerowah - League of Legends Skin Changer",
  "main": "./out/main/index.js",
  "author": "nerowah Team",
  "homepage": "https://github.com/nerowah/nerowah",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "fetch-champion-data": "bun run scripts/fetch-champion-data.ts",
    "fetch-champion-data:force": "bun run scripts/fetch-champion-data.ts --force"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "adm-zip": "^0.5.16",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-overlay-window": "^3.3.0",
    "electron-updater": "^6.3.9",
    "framer-motion": "^12.23.3",
    "i18next": "^25.2.1",
    "i18next-electron-fs-backend": "^3.0.3",
    "jotai": "^2.12.5",
    "lucide-react": "^0.525.0",
    "next-themes": "^0.4.6",
    "peerjs": "^1.5.5",
    "react-easy-sort": "^1.6.0",
    "react-i18next": "^15.5.3",
    "react-markdown": "^10.1.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^22.15.34",
    "@types/peerjs": "^1.1.0",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@types/react-window": "^1.8.8",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "electron": "^35.1.5",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "p-limit": "^6.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.8.3",
    "vite": "^6.2.6"
  }
}
